{
  "name": "autobinder",
  "version": "1.0.0",
  "description": "Automatically bind 'this' to every class method created outside of constructor",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha ./test/*.test.js",
    "coverage": "./node_modules/.bin/nyc npm run test",
    "report-coverage": "./node_modules/.bin/nyc report --reporter=text-lcov > coverage.lcov && ./node_modules/.bin/codecov"
  },
  "keywords": [
    "this",
    "autobinder",
    "class this binder"
  ],
  "homepage": "https://github.com/BalenD/AutoBinder",
  "author": "Balend https://github.com/BalenD",
  "license": "MIT",
  "devDependencies": {
    "chai": "4.2.0",
    "codecov": "^3.6.1",
    "eslint": "6.6.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "mocha": "6.2.2",
    "nyc": "14.1.1"
  }
}
